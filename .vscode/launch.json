{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/agate.js",
			"stopOnEntry": false,
			"args": ["-t", "-p", "./tests/myadventure.agate"],
